{:source-paths
 ["src"]

 :deps
 {:aliases [:cljs]}

 :nrepl
 {:port 8777}
 
 :builds
 {:app {:target     :browser
        :output-dir "dist"
        :asset-path "."

        :modules
        {:main
         {:entries [crinkle-bench.core]}}

        :devtools
        {:after-load  crinkle-bench.core/start
         :http-root   "."
         :http-port   8700
         :preloads    [devtools.preload]}}}}
